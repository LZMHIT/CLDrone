<?xml version="1.0" ?>
<sdf version="1.4">
  <model name="mbzirc">
    <static>true</static>

    <link name="field">
      <pose>0 0 0.01 0 0 0</pose>
      <collision name="collision">
        <geometry>
          <plane>
            <normal>0 0 1</normal>
            <size>100 60</size>
          </plane>
        </geometry>
        <surface>
          <friction>
            <ode>
              <mu>100</mu>  <!--为什么这么大?-->
              <mu2>50</mu2>
