        </surface>

      </collision>

      <visual name="carpet">
        <cast_shadows>false</cast_shadows>
        <geometry>
          <plane>
            <normal>0 0 1</normal>
            <size>100 60</size>
          </plane>
        </geometry>
	<material> 
          <script>
            <uri>file://media/materials/scripts/gazebo.material</uri> <!--系统自带的？-->
            <name>Gazebo/Black</name>
          </script>
	</material>
